/* EchoNote Light Theme - Teams Style */

/* Global Styles */
* {
    font-family: "Segoe UI", -apple-system, BlinkMacSystemFont, Roboto, "Helvetica Neue", Arial, sans-serif;
    font-size: 14px;
}

QMainWindow {
    background-color: #f3f2f1;
}

/* Section Title */
QLabel#section_title {
    font-size: 16px;
    font-weight: bold;
    color: #333333;
    padding: 5px 0px;
}

/* General Labels */
QLabel#engine_label {
    color: #333333;
    background-color: transparent;
}

QLabel#model_label {
    color: #333333;
    font-weight: 600;
    background-color: transparent;
}

QLabel#info_label {
    color: #6c757d;
    font-size: 12px;
    background-color: transparent;
}

/* Content Area Styles */
QWidget {
    background-color: #ffffff;
    color: #323130;
}

/* Labels */
QLabel {
    color: #323130;
    background-color: transparent;
}

/* Settings Dialog Labels */
QDialog QLabel {
    color: #212529;
    font-size: 13px;
    background-color: transparent;
}

#error_message {
    color: #d32f2f;
    font-weight: bold;
}

/* Generic button styles are centralized in unified override layer. */

QComboBox#model_combo,
QComboBox#engine_combo {
    min-width: 160px;
}

QListWidget#task_list {
    background-color: #ffffff;
    border: 1px solid #e1dfdd;
    border-radius: 6px;
    padding: 6px;
}

/* Generic text input styles are centralized in unified override layer. */

#error_details {
    background-color: #fff8f8;
    border: 1px solid #ffcccc;
    font-family: "Courier New", Courier, monospace;
    font-size: 12px;
}

/* Combo Boxes and Date Editors */
QComboBox, QDateEdit, QDateTimeEdit {
    background-color: #ffffff;
    color: #323130;
    border: 1px solid #8a8886;
    border-radius: 4px;
    padding: 8px 14px;
    padding-right: 28px;
    min-width: 120px;
    min-height: 32px;
    font-size: 14px;
    font-weight: 400;
}

QComboBox:hover, QDateEdit:hover, QDateTimeEdit:hover {
    background-color: #f3f2f1;
    border: 1px solid #2196F3;
}

QComboBox:focus, QDateEdit:focus, QDateTimeEdit:focus {
    background-color: #ffffff;
    border: 2px solid #2196F3;
    padding: 7px 13px;
    padding-right: 27px;
}

QComboBox:disabled, QDateEdit:disabled, QDateTimeEdit:disabled {
    background-color: #f3f2f1;
    color: #a19f9d;
    border: 1px solid #c8c6c4;
}

QComboBox::drop-down {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 24px;
    border-top: none;
    border-right: none;
    border-bottom: none;
    border-left: 1px solid #c8c6c4;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    background-color: transparent;
    padding: 0;
    margin: 0;
}

QComboBox::drop-down:hover {
    background-color: #f3f2f1;
    border-top: none;
    border-right: none;
    border-bottom: none;
    border-left: 1px solid #2196F3;
}

QComboBox::drop-down:pressed {
    background-color: #2196F3;
    border-top: none;
    border-right: none;
    border-bottom: none;
    border-left: 1px solid #2196F3;
}

QDateEdit::drop-down, QDateTimeEdit::drop-down {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 24px;
    background-color: transparent;
    border-top: none;
    border-right: none;
    border-bottom: none;
    border-left: 1px solid #c8c6c4;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    padding: 0;
    margin: 0;
}

QComboBox::down-arrow {
    image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='10' height='6' viewBox='0 0 10 6'><path d='M1 1L5 5L9 1' stroke='%23605e5c' stroke-width='1.4' fill='none' stroke-linecap='round' stroke-linejoin='round'/></svg>");
    width: 10px;
    height: 6px;
}

QComboBox::down-arrow:disabled {
    image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='10' height='6' viewBox='0 0 10 6'><path d='M1 1L5 5L9 1' stroke='%23a19f9d' stroke-width='1.4' fill='none' stroke-linecap='round' stroke-linejoin='round'/></svg>");
}

QDateEdit::down-arrow, QDateTimeEdit::down-arrow {
    image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='10' height='6' viewBox='0 0 10 6'><path d='M1 1L5 5L9 1' stroke='%23605e5c' stroke-width='1.4' fill='none' stroke-linecap='round' stroke-linejoin='round'/></svg>");
    width: 10px;
    height: 6px;
}

QDateEdit::down-arrow:disabled, QDateTimeEdit::down-arrow:disabled {
    image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='10' height='6' viewBox='0 0 10 6'><path d='M1 1L5 5L9 1' stroke='%23a19f9d' stroke-width='1.4' fill='none' stroke-linecap='round' stroke-linejoin='round'/></svg>");
}

/* Dialogs */
QDialog {
    background-color: #ffffff;
}

/* Tabs */
QTabWidget::pane {
    border: 1px solid #d0d0d0;
    background-color: #ffffff;
}

QTabBar::tab {
    background-color: #f0f0f0;
    color: #333333;
    border: 1px solid #d0d0d0;
    border-bottom: none;
    padding: 8px 16px;
    margin-right: 2px;
}

QTabBar::tab:selected {
    background-color: #ffffff;
    border-bottom: 2px solid #2196F3;
}

QTabBar::tab:hover {
    background-color: #e8e8e8;
}

/* ========================================
   Lists - General Styles
   ======================================== */

QListWidget, QTreeWidget, QTableWidget {
    background-color: #ffffff;
    color: #323130;
    border: 1px solid #e1dfdd;
    border-radius: 4px;
    padding: 4px;
}

QListWidget::item, QTreeWidget::item, QTableWidget::item {
    padding: 8px 12px;
    border-radius: 2px;
    margin: 1px 0px;
    border: 1px solid transparent;
}

QListWidget::item:selected, QTreeWidget::item:selected, QTableWidget::item:selected {
    background-color: #f3f2f1;
    color: #2196F3;
    border: 1px solid #2196F3;
}

QListWidget::item:hover, QTreeWidget::item:hover, QTableWidget::item:hover {
    background-color: #faf9f8;
    border: 1px solid #e1dfdd;
}

QListWidget::item:selected:hover, QTreeWidget::item:selected:hover, QTableWidget::item:selected:hover {
    background-color: #edebe9;
    color: #1976D2;
    border: 1px solid #1976D2;
}

/* Alternate row colors for better readability */
QListWidget::item:alternate, QTreeWidget::item:alternate, QTableWidget::item:alternate {
    background-color: #faf9f8;
}

/* Focus state */
QListWidget:focus::item:selected, QTreeWidget:focus::item:selected, QTableWidget:focus::item:selected {
    background-color: #edebe9;
    color: #1976D2;
    border: 2px solid #2196F3;
}

/* Spin Boxes */
QSpinBox, QDoubleSpinBox {
    background-color: #ffffff;
    color: #323130;
    border: 1px solid #8a8886;
    border-radius: 2px;
    padding: 8px 12px;
    padding-right: 32px;
    font-size: 14px;
    font-weight: 400;
    min-width: 80px;
    min-height: 32px;
}

QSpinBox:hover, QDoubleSpinBox:hover {
    background-color: #f3f2f1;
    border: 1px solid #605e5c;
}

QSpinBox:focus, QDoubleSpinBox:focus {
    background-color: #ffffff;
    border: 2px solid #2196F3;
    padding: 7px 11px;
    padding-right: 31px;
}

QSpinBox::up-button, QDoubleSpinBox::up-button {
    background-color: #ffffff;
    border-left: 1px solid #c8c6c4;
    border-bottom: 1px solid #c8c6c4;
    border-top-right-radius: 2px;
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    padding: 0;
    margin: 0;
}

QSpinBox::up-button:hover, QDoubleSpinBox::up-button:hover {
    background-color: #f3f2f1;
    border-left: 1px solid #2196F3;
    border-bottom: 1px solid #2196F3;
}

QSpinBox::up-button:pressed, QDoubleSpinBox::up-button:pressed {
    background-color: #2196F3;
    border-left: 1px solid #2196F3;
    border-bottom: 1px solid #2196F3;
}

QSpinBox::down-button, QDoubleSpinBox::down-button {
    background-color: #ffffff;
    border-left: 1px solid #c8c6c4;
    border-top: 1px solid #c8c6c4;
    border-bottom-right-radius: 2px;
    subcontrol-origin: padding;
    subcontrol-position: bottom right;
    width: 20px;
    padding: 0;
    margin: 0;
}

QSpinBox::down-button:hover, QDoubleSpinBox::down-button:hover {
    background-color: #f3f2f1;
    border-left: 1px solid #2196F3;
    border-top: 1px solid #2196F3;
}

QSpinBox::down-button:pressed, QDoubleSpinBox::down-button:pressed {
    background-color: #2196F3;
    border-left: 1px solid #2196F3;
    border-top: 1px solid #2196F3;
}

QSpinBox::up-arrow, QDoubleSpinBox::up-arrow {
    image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='9' height='6' viewBox='0 0 9 6'><path d='M1 5L4.5 1L8 5' stroke='%23605e5c' stroke-width='1.4' fill='none' stroke-linecap='round' stroke-linejoin='round'/></svg>");
    width: 9px;
    height: 6px;
}

QSpinBox::down-arrow, QDoubleSpinBox::down-arrow {
    image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='9' height='6' viewBox='0 0 9 6'><path d='M1 1L4.5 5L8 1' stroke='%23605e5c' stroke-width='1.4' fill='none' stroke-linecap='round' stroke-linejoin='round'/></svg>");
    width: 9px;
    height: 6px;
}

QSpinBox::up-arrow:disabled, QDoubleSpinBox::up-arrow:disabled {
    image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='9' height='6' viewBox='0 0 9 6'><path d='M1 5L4.5 1L8 5' stroke='%23a19f9d' stroke-width='1.4' fill='none' stroke-linecap='round' stroke-linejoin='round'/></svg>");
}

QSpinBox::down-arrow:disabled, QDoubleSpinBox::down-arrow:disabled {
    image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' width='9' height='6' viewBox='0 0 9 6'><path d='M1 1L4.5 5L8 1' stroke='%23a19f9d' stroke-width='1.4' fill='none' stroke-linecap='round' stroke-linejoin='round'/></svg>");
}

/* Sliders */
QSlider::groove:horizontal {
    background-color: #e0e0e0;
    height: 4px;
    border-radius: 2px;
}

QSlider::handle:horizontal {
    background-color: #2196F3;
    width: 16px;
    height: 16px;
    margin: -6px 0;
    border-radius: 8px;
}

QSlider::handle:horizontal:hover {
    background-color: #106ebe;
}

/* Tooltips */
QToolTip {
    background-color: #333333;
    color: #ffffff;
    border: none;
    border-radius: 4px;
    padding: 6px 10px;
}

/* Menu Bar */
QMenuBar {
    background-color: #ffffff;
    color: #333333;
    border-bottom: 1px solid #d0d0d0;
}

QMenuBar::item {
    padding: 6px 12px;
    background-color: transparent;
}

QMenuBar::item:selected {
    background-color: #e8f4fd;
}

QMenuBar::item:pressed {
    background-color: #d0e8f7;
}

/* Menus */
QMenu {
    background-color: #ffffff;
    color: #333333;
    border: 1px solid #d0d0d0;
    border-radius: 4px;
}

QMenu::item {
    padding: 8px 24px 8px 12px;
}

QMenu::item:selected {
    background-color: #e8f4fd;
}

QMenu::separator {
    height: 1px;
    background-color: #d0d0d0;
    margin: 4px 0;
}

/* Status Bar */
QStatusBar {
    background-color: #f0f0f0;
    color: #333333;
    border-top: 1px solid #d0d0d0;
}

/* ========================================
   Timeline Event Cards
   ======================================== */

/* Event card container */
#event_card {
    background-color: #ffffff;
    border: 1px solid #d8dde6;
    border-radius: 10px;
    padding: 8px;
}

#event_card:hover {
    background-color: #f7faff;
    border: 1px solid #2196F3;
}

/* Event card labels */
#event_card QLabel {
    background-color: transparent;
    color: #212529;
}

#event_card QLabel#time_label,
#event_card QLabel#detail_label {
    color: #5f6b7a;
    font-size: 11px;
}

QLabel[role="event-meta"] {
    color: #5f6b7a;
    font-size: 11px;
}

#event_card QLabel#description_label {
    color: #445164;
    font-size: 12px;
}

#event_card QLabel#no_artifacts_label {
    color: #adb5bd;
    font-style: italic;
    font-size: 12px;
}

/* Event card title */
#event_card QLabel[role="event-title"] {
    color: #212529;
    font-weight: 600;
    font-size: 13px;
}

/* Timeline action buttons are defined in unified override layer. */

/* Timeline Transcript Text Edit */
QTextEdit#timeline_transcript_text {
    background-color: #ffffff;
    color: #212529;
    border: 2px solid #e1dfdd;
    border-radius: 6px;
    padding: 12px;
    font-family: "SF Mono", "Monaco", "Consolas", "Courier New", Courier, monospace;
    font-size: 12px;
    line-height: 1.6;
    selection-background-color: #2196F3;
    selection-color: #ffffff;
}

/* Realtime Record Text Areas - Enhanced */

/* Transcription Text Area - Pure white background with improved readability */
QPlainTextEdit#transcription_text {
    background-color: #ffffff;
    color: #212529;
    border: 2px solid #e1dfdd;
    border-radius: 6px;
    padding: 12px;
    font-family: "SF Mono", "Monaco", "Consolas", "Courier New", Courier, monospace;
    font-size: 14px;
    line-height: 1.6;
    selection-background-color: #2196F3;
    selection-color: #ffffff;
}

/* Translation Text Area - Subtle background difference for visual distinction */
QPlainTextEdit#translation_text {
    background-color: #f3f2f1;
    color: #212529;
    border: 2px solid #e1dfdd;
    border-radius: 6px;
    padding: 12px;
    font-family: "SF Mono", "Monaco", "Consolas", "Courier New", Courier, monospace;
    font-size: 14px;
    line-height: 1.6;
    selection-background-color: #2196F3;
    selection-color: #ffffff;
}

/* Translation Text Area - Disabled State */
QPlainTextEdit#translation_text:disabled {
    background-color: #e9ecef;
    color: #6c757d;
    border: 2px solid #ced4da;
}

/* Focus States for Text Areas */
QPlainTextEdit#transcription_text:focus {
    background-color: #f3f2f1;
    border: 2px solid #2196F3;
}

QPlainTextEdit#translation_text:focus {
    background-color: #ffffff;
    border: 2px solid #2196F3;
}

/* Custom Scrollbars for Transcription Text Area */
QPlainTextEdit#transcription_text QScrollBar:vertical {
    background-color: #f3f2f1;
    width: 10px;
    border: none;
    border-radius: 5px;
}

QPlainTextEdit#transcription_text QScrollBar::handle:vertical {
    background-color: #adb5bd;
    border-radius: 5px;
    min-height: 30px;
}

QPlainTextEdit#transcription_text QScrollBar::handle:vertical:hover {
    background-color: #6c757d;
}

QPlainTextEdit#transcription_text QScrollBar::add-line:vertical,
QPlainTextEdit#transcription_text QScrollBar::sub-line:vertical {
    height: 0px;
}

QPlainTextEdit#transcription_text QScrollBar::add-page:vertical,
QPlainTextEdit#transcription_text QScrollBar::sub-page:vertical {
    background: none;
}

/* Custom Scrollbars for Translation Text Area */
QPlainTextEdit#translation_text QScrollBar:vertical {
    background-color: #f3f2f1;
    width: 10px;
    border: none;
    border-radius: 5px;
}

QPlainTextEdit#translation_text QScrollBar::handle:vertical {
    background-color: #adb5bd;
    border-radius: 5px;
    min-height: 30px;
}

QPlainTextEdit#translation_text QScrollBar::handle:vertical:hover {
    background-color: #6c757d;
}

QPlainTextEdit#translation_text QScrollBar::add-line:vertical,
QPlainTextEdit#translation_text QScrollBar::sub-line:vertical {
    height: 0px;
}

QPlainTextEdit#translation_text QScrollBar::add-page:vertical,
QPlainTextEdit#translation_text QScrollBar::sub-page:vertical {
    background: none;
}

/* Realtime Record Labels */
QLabel#input_label,
QLabel#gain_label,
QLabel#source_lang_label,
QLabel#target_lang_label {
    color: #333333;
    background-color: transparent;
}

/* Model Management Page Styles */

/* Model Cards - Downloaded */
QFrame#model-card-downloaded {
    background-color: #f0f7f0;
    border: 1px solid #c8e6c9;
    border-radius: 8px;
    padding: 15px;
}

QFrame#model-card-downloaded:hover {
    border: 2px solid #81c784;
    padding: 14px;
}

/* Model Cards - Available */
QFrame#model-card-available {
    background-color: #fafafa;
    border: 1px solid #e0e0e0;
    border-radius: 8px;
    padding: 15px;
}

QFrame#model-card-available:hover {
    border: 2px solid #2196F3;
    padding: 14px;
}

/* Recommendation Card */
QFrame#recommendation-card {
    background-color: #e3f2fd;
    border: 2px solid #2196f3;
    border-radius: 8px;
    padding: 20px;
}

QFrame#recommendation-card:hover {
    border: 2px solid #1976d2;
    background-color: #d1e7fd;
}

/* Model Card Progress Bars */
QFrame#model-card-available QProgressBar {
    background-color: #e0e0e0;
    border: none;
    border-radius: 4px;
    height: 20px;
    text-align: center;
    color: #333333;
    font-weight: bold;
}

QFrame#model-card-available QProgressBar::chunk {
    background-color: #2196F3;
    border-radius: 4px;
}

/* Model Card Buttons */
QFrame#model-card-downloaded QPushButton,
QFrame#model-card-available QPushButton {
    min-width: 80px;
    padding: 6px 12px;
}

/* Delete Button in Downloaded Cards */
QFrame#model-card-downloaded QPushButton[role="model-delete"] {
    background-color: transparent;
    color: #d32f2f;
    border: 1px solid #d32f2f;
}

QFrame#model-card-downloaded QPushButton[role="model-delete"]:hover {
    background-color: #d32f2f;
    color: #ffffff;
}

QFrame#model-card-downloaded QPushButton[role="model-delete"]:disabled {
    background-color: transparent;
    color: #cccccc;
    border: 1px solid #cccccc;
}

/* Download Button in Available Cards */
QFrame#model-card-available QPushButton[role="model-download"] {
    background-color: #2196F3;
    color: #ffffff;
}

QFrame#model-card-available QPushButton[role="model-download"]:hover {
    background-color: #106ebe;
}

/* Recommendation Card Download Button */
QFrame#recommendation-card QPushButton {
    background-color: #2196f3;
    color: #ffffff;
    font-weight: bold;
    padding: 10px 20px;
    border-radius: 5px;
    border: none;
    min-width: 150px;
}

QFrame#recommendation-card QPushButton:hover {
    background-color: #1976d2;
}

QFrame#recommendation-card QPushButton:pressed {
    background-color: #0d47a1;
}

/* Model Details Dialog */
QDialog#model_config_dialog QLabel {
    color: #333333;
}

QDialog#model_config_dialog QLabel[role="model-config-field-label"] {
    font-weight: bold;
    color: #666666;
}

/* Model Config Dialog */
QDialog QFormLayout QLabel {
    color: #333333;
    font-weight: normal;
}

QDialog QCheckBox {
    color: #333333;
}

QDialog QSpinBox, QDialog QDoubleSpinBox {
    min-width: 120px;
    max-width: 200px;
}

/* Model Management Description */
QLabel#description_label {
    color: #666666;
    margin-bottom: 10px;
}

/* Model Card Labels */
QFrame#model-card-downloaded QLabel,
QFrame#model-card-available QLabel {
    background-color: transparent;
}

/* Recommendation card labels */
QFrame#recommendation-card QLabel {
    background-color: transparent;
}

/* FFmpeg Installation Dialog */
QTextEdit#ffmpeg_instructions_text {
    background-color: #f3f2f1;
    color: #333333;
    border: 1px solid #e1dfdd;
    border-radius: 4px;
    padding: 10px;
    font-family: "Courier New", Courier, monospace;
    font-size: 12px;
}

QLabel#ffmpeg_note_label {
    color: #666666;
    font-style: italic;
}

QTextEdit#loopback_instructions_text {
    background-color: #f3f2f1;
    color: #333333;
    border: 1px solid #d0d0d0;
    border-radius: 4px;
    padding: 10px;
    font-family: "Courier New", Courier, monospace;
    font-size: 12px;
}

QLabel#loopback_note_label {
    color: #666666;
    font-style: italic;
}

QLabel#loopback_admin_note_label {
    background-color: #fff8f0;
    color: #8d5524;
    border: 1px solid #ffcc80;
    border-radius: 6px;
    padding: 8px 10px;
    font-size: 12px;
}

/* Transcript Viewer Dialog */

/* Metadata Frame - Enhanced with better visual hierarchy */
QFrame#metadata_frame {
    background-color: #fafafa;
    border-bottom: 1px solid #e0e0e0;
    padding: 20px;
}

QFrame#metadata_frame QLabel {
    color: #333333;
    background-color: transparent;
}

/* Toolbar Frame - Unified button styling */
QFrame[role="batch-viewer-toolbar"] {
    background-color: #ffffff;
    border-bottom: 1px solid #e0e0e0;
    padding: 12px 20px;
}

/* Text Frame */
QFrame#text_frame {
    background-color: #ffffff;
    padding: 20px;
}

/* Transcript Text Edit - Enhanced visual feedback */
QTextEdit#transcript_text_edit {
    background-color: #ffffff;
    color: #212529;
    border: 2px solid #e1dfdd;
    border-radius: 8px;
    padding: 16px;
    font-family: "SF Mono", "Monaco", "Consolas", "Courier New", Courier, monospace;
    font-size: 14px;
    line-height: 1.8;
    selection-background-color: #2196F3;
    selection-color: #ffffff;
}

QTextEdit#transcript_text_edit:focus {
    border: 2px solid #2196F3;
    background-color: #fafafa;
}

QTextEdit#transcript_text_edit:read-only {
    background-color: #f3f2f1;
    border: 2px solid #e0e0e0;
}

/* Search Widget - Harmonized with overall theme */
