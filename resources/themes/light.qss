QDateEdit:focus,
QDateTimeEdit:focus,
QSpinBox:focus,
QDoubleSpinBox:focus {
    border: 2px solid #1d63e9;
    padding: 3px 8px;
}

QComboBox::drop-down,
QDateEdit::drop-down,
QDateTimeEdit::drop-down,
QSpinBox::up-button,
QSpinBox::down-button,
QDoubleSpinBox::up-button,
QDoubleSpinBox::down-button {
    border: none;
    width: 18px;
    background-color: transparent;
}

QComboBox QAbstractItemView {
    border: 1px solid #c8d5e9;
    border-radius: 6px;
    padding: 2px;
    outline: none;
}

QComboBox QAbstractItemView::item {
    min-height: 18px;
    padding: 3px 7px;
}

QComboBox QAbstractItemView::item:hover {
    background-color: #edf4ff;
}

QComboBox QAbstractItemView::item:selected {
    background-color: #1d63e9;
    color: #ffffff;
    font-weight: 600;
}

QListWidget,
QTableView,
QTreeView {
    background-color: #ffffff;
    color: #1f2d44;
    border: 1px solid #d1dced;
    border-radius: 8px;
    padding: 4px;
    selection-background-color: #e7f0ff;
    selection-color: #12408f;
    alternate-background-color: #f8fbff;
}

QHeaderView::section {
    background-color: #f4f8ff;
    color: #334b6f;
    border: none;
    border-right: 1px solid #d8e2f0;
    border-bottom: 1px solid #d8e2f0;
    padding: 8px 10px;
    font-weight: 600;
}

QGroupBox {
    margin-top: 12px;
    padding: 14px 12px 12px 12px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    left: 10px;
    padding: 0 4px;
    color: #234270;
    font-weight: 700;
}

QGroupBox[role="settings-config-group"] {
    margin-top: 8px;
    padding: 10px 10px 8px 10px;
    border-radius: 9px;
}

QGroupBox[role="settings-config-group"]::title {
    left: 8px;
    padding: 0 3px;
}

QGroupBox[role="settings-config-group-nested"] {
    margin-top: 6px;
    padding: 8px 9px 7px 9px;
    border-radius: 8px;
}

QGroupBox[role="settings-config-group-nested"]::title {
    left: 8px;
    padding: 0 3px;
}

QLabel[role="realtime-duration"] {
    font-size: 14px;
    font-weight: 600;
    color: #1d63e9;
    padding: 0 8px;
    min-height: 28px;
}

QLabel[role="audio-player-time"] {
    color: #5c6f8f;
    font-size: 12px;
    font-family: "SF Mono", "Consolas", "Monaco", "Courier New", monospace;
    font-weight: 500;
}

QPushButton[role="audio-player-primary"] {
    background-color: #f3f2f1;
    color: #212529;
    border: 2px solid #e1dfdd;
    border-radius: 999px;
    padding: 0;
    min-width: 0;
}

QPushButton[role="audio-player-primary"]:hover {
    background-color: #e9ecef;
    border-color: #2196F3;
}

QPushButton[role="audio-player-primary"]:pressed {
    background-color: #e1dfdd;
    border-color: #1976D2;
}

QPushButton[role="audio-player-primary"]:disabled {
    background-color: #f3f2f1;
    color: #adb5bd;
    border-color: #e9ecef;
}

QPushButton[role="audio-player-control"] {
    background-color: #f3f2f1;
    color: #212529;
    border: 1px solid #e1dfdd;
    border-radius: 999px;
    padding: 0;
    min-width: 0;
}

QPushButton[role="audio-player-control"]:hover {
    background-color: #e9ecef;
    border-color: #ced4da;
}

QPushButton[role="audio-player-control"]:pressed {
    background-color: #e1dfdd;
    border-color: #2196F3;
}

QPushButton[role="audio-player-control"]:checked {
    background-color: #e3f2fd;
    border-color: #2196F3;
    color: #1976D2;
}

QPushButton[role="audio-player-control"]:disabled {
    background-color: #f3f2f1;
    color: #adb5bd;
    border-color: #e9ecef;
}

QSlider[role="audio-player-progress"] {
    min-height: 22px;
}

QSlider[role="audio-player-progress"]::groove:horizontal {
    background: #e1dfdd;
    height: 4px;
    border-radius: 2px;
    border: none;
}

QSlider[role="audio-player-progress"]::handle:horizontal {
    background: #2196F3;
    width: 14px;
    height: 14px;
    margin: -5px 0;
    border-radius: 7px;
}

QSlider[role="audio-player-progress"]::handle:horizontal:hover {
    background: #42a5f5;
    width: 16px;
    height: 16px;
    margin: -6px 0;
}

QSlider[role="audio-player-progress"]::handle:horizontal:pressed {
    background: #1d63e9;
    width: 16px;
    height: 16px;
    margin: -6px 0;
    border: none;
}

QSlider[role="audio-player-progress"]::sub-page:horizontal {
    background: #2196F3;
    border-radius: 2px;
}

QSlider[role="audio-player-progress"]::add-page:horizontal {
    background: #e1dfdd;
    border-radius: 2px;
}

QSlider[role="audio-player-progress"]:disabled::groove:horizontal {
    background: #e9ecef;
}

QSlider[role="audio-player-progress"]:disabled::handle:horizontal {
    background: #adb5bd;
}

QSlider[role="audio-player-volume"] {
    min-height: 20px;
    max-height: 30px;
    min-width: 100px;
    max-width: 100px;
}

QSlider[role="audio-player-volume"]::groove:horizontal {
    background: #e1dfdd;
    height: 3px;
    border-radius: 1px;
    border: none;
}

QSlider[role="audio-player-volume"]::handle:horizontal {
    background: #2196F3;
    width: 12px;
    height: 12px;
    margin: -4px 0;
    border-radius: 6px;
}

QSlider[role="audio-player-volume"]::handle:horizontal:hover {
    background: #42a5f5;
    width: 14px;
    height: 14px;
    margin: -5px 0;
}

QSlider[role="audio-player-volume"]::handle:horizontal:pressed {
    background: #1d63e9;
    width: 14px;
    height: 14px;
    margin: -5px 0;
    border: none;
}

QSlider[role="audio-player-volume"]::sub-page:horizontal {
    background: #2196F3;
    border-radius: 1px;
}

QSlider[role="audio-player-volume"]::add-page:horizontal {
    background: #e1dfdd;
    border-radius: 1px;
}

QProgressBar {
    background-color: #edf2fb;
    border: 1px solid #d5deed;
    border-radius: 7px;
    text-align: center;
    color: #234270;
}

QProgressBar::chunk {
    background-color: #1d63e9;
    border-radius: 6px;
}

QCheckBox,
QRadioButton {
    spacing: 8px;
    color: #1f2d44;
}

QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #9fb2d1;
    background-color: #ffffff;
    border-radius: 4px;
}

QRadioButton::indicator {
    width: 16px;
    height: 16px;
    border: 1px solid #9fb2d1;
    background-color: #ffffff;
    border-radius: 8px;
}

QCheckBox::indicator:checked,
QRadioButton::indicator:checked {
    border-color: #1d63e9;
    background-color: #1d63e9;
}

QTabWidget#main_tabs QTabBar::tab {
    background-color: #f5f8ff;
    color: #4b5f82;
    border: 1px solid #d7e1f0;
    border-bottom: none;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    padding: 9px 16px;
}

QTabWidget#main_tabs QTabBar::tab:selected {
    background-color: #ffffff;
    color: #153766;
    border-color: #c5d5eb;
}

QTabWidget#main_tabs QTabBar::tab:hover:!selected {
    background-color: #edf4ff;
    color: #284a79;
}

QListWidget[role="settings-nav"] {
    background-color: #f7faff;
    border: 1px solid #d7e1f0;
    border-radius: 10px;
    padding: 2px;
}

QListWidget[role="settings-nav"]::item {
    border-radius: 7px;
    padding: 5px 7px;
    color: #2e466c;
}

QListWidget[role="settings-nav"]::item:selected {
    background-color: #e8f1ff;
    color: #12408f;
    font-weight: 700;
}

QLabel[role="oauth-status"][state="success"],
QLabel[role="ffmpeg-status"][state="success"],
QLabel[role="task-status"][state="completed"] {
    color: #1e8c4b;
}

QLabel[role="oauth-status"][state="error"],
QLabel[role="ffmpeg-status"][state="missing"],
QLabel[role="task-status"][state="failed"] {
    color: #c93333;
}

QLabel[role="task-status"][state="processing"] {
    color: #1d63e9;
}

QLabel[role="task-status"][state="pending"],
QLabel[role="task-status"][state="cancelled"],
QLabel[role="sync-status"],
QLabel[role="device-info"],
QLabel[role="auto-start-desc"],
QLabel[role="time-display"] {
    color: #5a6f93;
}

QScrollBar:vertical {
    background-color: #f0f4fb;
    width: 12px;
    margin: 2px;
    border-radius: 6px;
}

QScrollBar::handle:vertical {
    background-color: #c2d2ea;
    min-height: 28px;
    border-radius: 6px;
}

QScrollBar::handle:vertical:hover {
    background-color: #a7bedf;
}

QScrollBar:horizontal {
    background-color: #f0f4fb;
    height: 12px;
    margin: 2px;
    border-radius: 6px;
}

QScrollBar::handle:horizontal {
    background-color: #c2d2ea;
    min-width: 28px;
    border-radius: 6px;
}

QScrollBar::add-line,
QScrollBar::sub-line {
    width: 0;
    height: 0;
}

QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
    background: none;
}
